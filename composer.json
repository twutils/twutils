{
    "name": "twutils/twutils",
    "description": "TwUtils :D",
    "keywords": [
        "twitter",
        "twitter-utilities"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "abraham/twitteroauth": "^2.0.1",
        "adrienrn/php-mimetyper": "^0.2.1",
        "beyondcode/laravel-dump-server": "^1.2",
        "doctrine/dbal": "^2.10",
        "fakerphp/faker": "^1.13",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.4",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/sail": "^1.3",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/commonmark": "^1.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1.26",
        "nelexa/zip": "dev-patch-1",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.3.1",
        "spatie/laravel-sluggable": "^2.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "brianium/paratest": "^6.2",
        "facade/ignition": "^2.3.6",
        "filp/whoops": "~2.0",
        "matt-allan/laravel-code-style": "^0.6.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6.13",
        "phpunit/phpunit": "^9.3",
        "symfony/thanks": "^1.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mohannadnaj-forks/php-zip.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "testData": [
            "phpunit --no-coverage --configuration testData.phpunit.xml"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
